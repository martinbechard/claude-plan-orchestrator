ass after the parse_task_usage() function.\\n\\\n   136→      \\   The class needs these methods:\\n\\n   class PlanUsageTracker:\\n       \\\"\\\"\\\n   137→      \\\"Accumulates token usage across all tasks in a plan run.\\\"\\\"\\\"\\n\\n       def\\\n   138→      \\ __init__(self):\\n           self.task_usages: dict[str, TaskUsage] = {}\\n\\n\\\n   139→      \\       def record(self, task_id: str, usage: TaskUsage) -> None:\\n        \\\n   140→      \\   \\\"\\\"\\\"Record usage for a completed task.\\\"\\\"\\\"\\n           self.task_usages[task_id]\\\n   141→      \\ = usage\\n\\n       def get_section_usage(self, plan: dict, section_id: str)\\\n   142→      \\ -> TaskUsage:\\n           \\\"\\\"\\\"Aggregate usage for all tasks in a given section.\\\"\\\n   143→      \\\"\\\"\\n           # Find the section, iterate its tasks, sum up usage for matching\\\n   144→      \\ task_ids\\n           total = TaskUsage()\\n           for section in plan.get(\\\"\\\n   145→      sections\\\", []):\\n               if section.get(\\\"id\\\") == section_id:\\n   \\\n   146→      \\                for task in section.get(\\\"tasks\\\", []):\\n                 \\\n   147→      \\      tid = task.get(\\\"id\\\", \\\"\\\")\\n                       if tid in self.task_usages:\\n\\\n   148→      \\                           u = self.task_usages[tid]\\n                    \\\n   149→      \\       total.input_tokens += u.input_tokens\\n                           total.output_tokens\\\n   150→      \\ += u.output_tokens\\n                           total.cache_read_tokens +=\\\n   151→      \\ u.cache_read_tokens\\n                           total.cache_creation_tokens\\\n   152→      \\ += u.cache_creation_tokens\\n                           total.total_cost_usd\\\n   153→      \\ += u.total_cost_usd\\n                           total.num_turns += u.num_turns\\n\\\n   154→      \\                           total.duration_api_ms += u.duration_api_ms\\n   \\\n   155→      \\        return total\\n\\n       def get_total_usage(self) -> TaskUsage:\\n  \\\n   156→      \\         \\\"\\\"\\\"Aggregate usage across all recorded tasks.\\\"\\\"\\\"\\n         \\\n   157→      \\  total = TaskUsage()\\n           for u in self.task_usages.values():\\n   \\\n   158→      \\            total.input_tokens += u.input_tokens\\n               total.output_tokens\\\n   159→      \\ += u.output_tokens\\n               total.cache_read_tokens += u.cache_read_tokens\\n\\\n   160→      \\               total.cache_creation_tokens += u.cache_creation_tokens\\n   \\\n   161→      \\            total.total_cost_usd += u.total_cost_usd\\n               total.num_turns\\\n   162→      \\ += u.num_turns\\n               total.duration_api_ms += u.duration_api_ms\\n\\\n   163→      \\           return total\\n\\n       def get_cache_hit_rate(self) -> float:\\n\\\n   164→      \\           \\\"\\\"\\\"Calculate overall cache hit rate.\\\"\\\"\\\"\\n           total\\\n   165→      \\ = self.get_total_usage()\\n           denom = total.cache_read_tokens + total.input_tokens\\n\\\n   166→      \\           return total.cache_read_tokens / denom if denom > 0 else 0.0\\n\\n\\\n   167→      \\       def format_summary_line(self, task_id: str) -> str:\\n           \\\"\\\"\\\n   168→      \\\"Format a one-line usage summary for a task.\\\"\\\"\\\"\\n           u = self.task_usages.get(task_id)\\n\\\n   169→      \\           if not u:\\n               return \\\"\\\"\\n           total = self.get_total_usage()\\n\\\n   170→      \\           cache_denom = u.cache_read_tokens + u.input_tokens\\n           cache_pct\\\n   171→      \\ = (u.cache_read_tokens / cache_denom * 100) if cache_denom > 0 else 0\\n  \\\n   172→      \\         return (\\n               f\\\"[Usage] Task {task_id}: ${u.total_cost_usd:.4f}\\\n   173→      \\ | \\\"\\n               f\\\"{u.input_tokens:,} in / {u.output_tokens:,} out /\\\n   174→      \\ \\\"\\n               f\\\"{u.cache_read_tokens:,} cached ({cache_pct:.0f}% cache\\\n   175→      \\ hit) | \\\"\\n               f\\\"Running: ${total.total_cost_usd:.4f}\\\"\\n    \\\n   176→      \\       )\\n\\n       def format_final_summary(self, plan: dict) -> str:\\n   \\\n   177→      \\        \\\"\\\"\\\"Format the final usage summary printed after all tasks complete.\\\"\\\n   178→      \\\"\\\"\\n           total = self.get_total_usage()\\n           cache_rate = self.get_cache_hit_rate()\\n\\\n   179→      \\           lines = [\\n               \\\"\\\\n=== Usage Summary ===\\\",\\n      \\\n   180→      \\         f\\\"Total cost: ${total.total_cost_usd:.4f}\\\",\\n               f\\\"\\\n   181→      Total tokens: {total.input_tokens:,} input / {total.output_tokens:,} output\\\"\\\n   182→      ,\\n               f\\\"Cache: {total.cache_read_tokens:,} read / {total.cache_creation_tokens:,}\\\n   183→      \\ created ({cache_rate:.0%} hit rate)\\\",\\n               f\\\"API time: {total.duration_api_ms\\\n   184→      \\ / 1000:.1f}s across {total.num_turns} turns\\\",\\n               \\\"Per-section\\\n   185→      \\ breakdown:\\\",\\n           ]\\n           for section in plan.get(\\\"sections\\\"\\\n   186→      , []):\\n               sid = section.get(\\\"id\\\", \\\"\\\")\\n               sname\\\n   187→      \\ = section.get(\\\"name\\\", sid)\\n               su = self.get_section_usage(plan,\\\n   188→      \\ sid)\\n               task_count = sum(\\n                   1 for t in section.get(\\\"\\\n   189→      tasks\\\", []) if t.get(\\\"id\\\") in self.task_usages\\n               )\\n      \\\n   190→      \\         if task_count > 0:\\n                   lines.append(f\\\"  {sname}:\\\n   191→      \\ ${su.total_cost_usd:.4f} ({task_count} tasks)\\\")\\n           return \\\"\\\\n\\\"\\\n   192→      .join(lines)\\n\\n2. Verify with: python3 -c \\\"import py_compile; py_compile.compile('scripts/plan-orchestrator.py',\\\n   193→      \\ doraise=True); print('syntax OK')\\\