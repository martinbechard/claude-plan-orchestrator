turn config if isinstance(config, dict) else {}\n    54→    except (IOError, yaml.YAMLError):\n    55→        return {}\n    56→\n    57→\n    58→_config = load_orchestrator_config()\n    59→DEV_SERVER_PORT = int(_config.get(\"dev_server_port\", DEFAULT_DEV_SERVER_PORT))\n    60→\n    61→# Known locations for the claude binary\n    62→CLAUDE_BINARY_SEARCH_PATHS = [\n    63→    \"/opt/homebrew/lib/node_modules/@anthropic-ai/claude-code/cli.js\",\n    64→    \"/usr/local/lib/node_modules/@anthropic-ai/claude-code/cli.js\",\n    65→]\n    66→\n    67→# Rate limit detection\n    68→RATE_LIMIT_DEFAULT_WAIT_SECONDS = 3600  # 1 hour fallback if we can't parse reset time\n    69→RATE_LIMIT_PATTERN = re.compile(\n    70→    r\"(?:You've hit your limit|you've hit your limit|Usage limit reached)\"\n    71→    r\".*?resets?\\s+(\\w+\\s+\\d{1,2})\\s+at\\s+(\\d{1,2}(?::\\d{2})?\\s*(?:am|pm|AM|PM)?)\"\n    72→    r\"(?:\\s*\\(([^)]+)\\))?\",\n    73→    re.IGNORECASE | re.DOTALL,\n    74→)\n    75→# Month name mapping for parsing\n    76→MONTH_NAMES = {\n    77→    \"jan\": 1, \"january\": 1, \"feb\": 2, \"february\": 2,\n    78→    \"mar\": 3, \"march\": 3, \"apr\": 4, \"april\": 4,\n    79→    \"may\": 5, \"jun\": 6, \"june\": 6,\n    80→    \"jul\": 7, \"july\": 7, \"aug\": 8, \"august\": 8,\n    81→    \"sep\": 9, \"september\": 9, \"oct\": 10, \"october\": 10,\n    82→    \"nov\": 11, \"november\": 11, \"dec\": 12, \"december\": 12,\n    83→}\n    84→\n    85→# Global verbose flag\n    86→VERBOSE = False\n    87→\n    88→# Resolved claude command (set at startup)\n    89→CLAUDE_CMD: list[str] = [\"claude\"]\n    90→\n    91→# Environment variables to strip from child Claude processes\n    92→# CLAUDECODE is set by Claude Code to detect nested sessions; we must remove it\n    93→# so the orchestrator can spawn Claude from within a Claude Code session.\n    94→STRIPPED_ENV_VARS = [\"CLAUDECODE\"]\n    95→\n    96→\n    97→def build_child_env() -> dict[str, str]:\n    98→    \"\"\"Build a clean environment for spawning Claude child processes.\"\"\"\n    99→    env = os.environ.copy()\n   100→    for var in STRIPPED_ENV_VARS:\n   101→        env.pop(var, None)\n   102→    return env\n   103→\n   104→\n   105→def verbose_log(message: str, prefix: str = \"VERBOSE\") -> None:\n   106→    \"\"\"Print a verbose log message if verbose mode is enabled.\"\"\"\n   107→    if VERBOSE:\n   108→        timestamp = datetime.now().strftime(\"%H:%M:%S.%f\")[:-3]\n   109→        print(f\"[{timestamp}] [{prefix}] {message}\", flush=True)\n   110→\n   111→\n   112→def resolve_claude_binary() -> list[str]:\n   113→    \"\"\"Find the claude binary, checking PATH then known install locations.\n   114→\n   115→    Returns a command list (e.g. ['claude'] or ['node', '/path/to/cli.js']).\n   116→    \"\"\"\n   117→    # First check if 'claude' is directly in PATH\n   118→    claude_path = shutil.which(\"claude\")\n   119→    if claude_path:\n   120→        return [claude_path]\n   121→\n   122→    # Check known installation paths (npm global installs)\n   123→    for search_path in CLAUDE_BINARY_SEARCH_PATHS:\n   124→        if os.path.isfile(search_path):\n   125→            node_path = shutil.which(\"node\")\n   126→            if node_path:\n   127→                return [node_path, search_path]\n   128→\n   129→    # Last resort: try npx\n   130→    npx_path = shutil.which(\"npx\")\n   131→    if npx_path:\n   132→        return [npx_path, \"@anthropic-ai/claude-code\"]\n   133→\n   134→    # Fallback - will fail at runtime with clear error\n   135→    print(\"[WARNING] Could not find 'claude' binary. Tasks will fail.\")\n   136→    print(\"[WARNING] Install with: npm install -g @anthropic-ai/claude-code\")\n   137→    return [\"claude\"]\n   138→\n   139→\n   140→def check_stop_requested() -> bool:\n   141→    \"\"\"Check if a graceful stop has been requested via semaphore file.\n   142→\n   143→    The orchestrator checks for the file at .claude/plans/.stop before\n   144→    starting each new task. If the file exists, the orchestrator will\n   145→    finish the current task but not start any new ones.\n   146→\n   147→    To request a stop:  touch .claude/plans/.stop\n   148→    The file is auto-cleaned when the orchestrator starts.\n   149→    \"\"\"\n   150→    if os.path.exists(STOP_SEMAPHORE_PATH):\n   151→        verbose_log(\"Stop semaphore detected\", \"STOP\")\n   152→        return True\n   153→    return False\n   154→\n   155→\n   156→def clear_stop_semaphore() -> None:\n   157→    \"\"\"Remove the stop semaphore file if it exists (called on startup).\"\"\"\n   158→    if os.path.exists(STOP_SEMAPHORE_PATH):\n   159→        os.remove(STOP_SEMAPHORE_PATH)\n   160→        print(f\"[Cleared stale stop semaphore: {STOP_SEMAPHORE_PATH}]\")\n   161→\n   162→\n   163→# Circuit breaker configuration\n   164→DEFAULT_CIRCUIT_BREAKER_THRESHOLD = 3  # consecutive failures to trip\n   165→DEFAULT_CIRCUIT_BREAKER_RESET_TIMEOUT = 300  # 5 minutes before retry\n   166→DEFAULT_BACKOFF_BASE = 5  # base seconds for exponential backoff\n   167→DEFAULT_BACKOFF_MAX = 120  # max backoff seconds\n   168→\n   169→\n   170→@dataclass\n   171→class TaskResult:\n   172→    \"\"\"Result of a task execution.\"\"\"\n   173→    success: bool\n   174→    message: str\n   175→    duration_seconds: float\n   176→    plan_modified: bool = False\n   177→    rate_limited: bool = False\n   178→    rate_limit_reset_time: Optional[datetime] = None\n   179→\n   180→\n   181→class CircuitBreaker:\n   182→    \"\"\"Circuit breaker to prevent runaway failures when LLM is unavailable.\"\"\"\n   183→\n   184→    def __init__(\n   185→        self,\n   186→        threshold: int = DEFAULT_CIRCUIT_BREAKER_THRESHOLD,\n   187→        reset_timeout: int = DEFAULT_CIRCUIT_BREAKER_RESET_TIMEOUT,\n   188→        backoff_base: int = DEFAULT_BACKOFF_BASE,\n   189→        backoff_max: int = DEFAULT_BACKOFF_MAX\n   190→    ):\n   191→        self.threshold = threshold\n   192→        self.reset_timeout = reset_timeout\n   193→        self.backoff_base = backoff_base\n   194→        self.backoff_max = backoff_max\n   195→        self.consecutive_failures = 0\n   196→        self.last_failure_time: Optional[float] = None\n   197→        self.is_open = False\n   198→\n   199→    def record_success(self) -> None:\n   200→        \"\"\"Record a successful task execution.\"\"\"\n   201→        self.consecutive_failures = 0\n   202→        self.is_open = False\n   203→        self.last_failure_time = None\n   204→\n   205→    def record_failure(self) -> None:\n   206→        \"\"\"Record a failed task execution.\"\"\"\n   207→        self.consecutive_failures += 1\n   208→        self.last_failure_time = time.time()\n   209→\n   210→        if self.consecutive_failures >= self.threshold:\n   211→            self.is_open = True\n   212→            print(f\"\\n[CIRCUIT BREAKER] Tripped after {self.consecutive_failures} consecutive failures\")\n   213→\n   214→    def can_proceed(self) -> bool:\n   215→        \"\"\"Check if we can proceed with another task.\"\"\"\n   216→        if not self.is_open:\n   217→            return True\n   218→\n   219→        # Check if enough time has passed to try again\n   220→        if self.last_failure_time:\n   221→            elapsed = time.time() - self.last_failure_time\n   222→            if elapsed >= self.reset_timeout:\n   223→                print(f\"[CIRCUIT BREAKER] Reset timeout elapsed ({self.reset_timeout}s), attempting recovery\")\n   224→                self.is_open = False\n   225→                self.consecutive_failures = 0\n   226→                return True\n   227→\n   228→        return False\n   229→\n   230→    def get_backoff_delay(self, attempt: int) -> float:\n   231→        \"\"\"Calculate exponential backoff delay.\"\"\"\n   232→        delay = min(self.backoff_base * (2 ** (attempt - 1)), self.backoff_max)\n   233→        return delay\n   234→\n   235→    def wait_for_reset(self) -> bool:\n   236→        \"\"\"Wait for circuit breaker reset timeout. Returns True if should continue.\"\"\"\n   237→        if not self.is_open or not self.last_failure_time:\n   238→            return True\n   239→\n   240→        elapsed = time.time() - self.last_failure_time\n   241→        remaining = self.reset_timeout - elapsed\n   242→\n   243→        if remaining > 0:\n   244→            print(f\"[CIRCUIT BREAKER] Open - waiting {remaining:.0f}s before retry...\")\n   245→            print(f\"[CIRCUIT BREAKER] Press Ctrl+C to abort\")\n   246→            try:\n   247→                time.sleep(remaining)\n   248→            except KeyboardInterrupt:\n   249→                print(\"\\n[CIRCUIT BREAKER] Aborted by user\")\n   250→                return False\n   251→\n   252→        self.is_open = False\n   253→        self.consecutive_failures = 0\n   254→        return True\n   255→\n   256→\n   257→def parse_rate_limit_reset_time(output: str) -> Optional[datetime]:\n   258→    \"\"\"Parse a rate limit reset time from Claude CLI output.\n   259→\n   260→    Recognizes messages like:\n   261→      \"You've hit your limit · resets Feb 9 at 6pm (America/Toronto)\"\n   262→      \"You've hit your limit · resets February 9 at 6:30pm (America/Toronto)\"\n   263→\n   264→    Returns a timezone-aware datetime if parseable, None otherwise.\n   265→    \"\"\"\n   266→    match = RATE_LIMIT_PATTERN.search(output)\n   267→    if not match:\n   268→        return None\n   269→\n   270→    date_str = match.group(1).strip()      # e.g. \"Feb 9\"\n   271→    time_str = match.group(2).strip()      # e.g. \"6pm\" or \"6:30pm\"\n   272→    tz_str = match.group(3)                # e.g. \"America/Toronto\" or None\n   273→\n   274→    try:\n   275→        # Parse month and day\n   276→        parts = date_str.split()\n   277→        month_name = parts[0].lower()\n   278→        day = int(parts[1])\n   279→        month = MONTH_NAMES.get(month_name)\n   280→        if month is None:\n   281→            print(f\"[RATE LIMIT] Could not parse month: {month_name}\")\n   282→            return None\n   283→\n   284→        # Parse time - handle \"6pm\", \"6:30pm\", \"18:00\"\n   285→        time_str_lower = time_str.lower().strip()\n   286→        hour = 0\n   287→        minute = 0\n   288→\n   289→        if \"am\" in time_str_lower or \"pm\" in time_str_lower:\n   290→            is_pm = \"pm\" in time_str_lower\n   291→            time_digits = time_str_lower.replace(\"am\", \"\").replace(\"pm\", \"\").strip()\n   292→            if \":\" in time_digits:\n   293→                hour_str, min_str = time_digits.split(\":\")\n   294→                hour = int(hour_str)\n   295→                minute = int(min_str)\n   296→            else:\n   297→                hour = int(time_digits)\n   298→            if is_pm and hour != 12:\n   299→                hour += 12\n   300→            elif not is_pm and hour == 12:\n   301→                hour = 0\n   302→        elif \":\" in time_str_lower:\n   303→            hour_str, min_str = time_str_lower.split(\":\")\n   304→            hour = int(hour_str)\n   305→            minute = int(min_str)\n   306→        else:\n   307→            hour = int(time_str_lower)\n   308→\n   309→        # Parse timezone\n   310→        tz = ZoneInfo(\"UTC\")\n   311→        if tz_str:\n   312→            try:\n   313→                tz = ZoneInfo(tz_str.strip())\n   314→            except (KeyError, ValueError):\n   315→                print(f\"[RATE LIMIT] Unknown timezone '{tz_str}', using UTC\")\n   316→\n   317→        # Build the datetime - use current year, handle year rollover\n   318→        now = datetime.now(tz)\n   319→        reset_time = now.replace(month=month, day=day, hour=hour, minute=minute, second=0, microsecond=0)\n   320→\n   321→        # If the reset time is in the past, it might be next year\n   322→        if reset_time < now - timedelta(hours=1):\n   323→            reset_time = reset_time.replace(year=now.year + 1)\n   324→\n   325→        return reset_time\n   326→\n   327→    except (ValueError, IndexError) as e:\n   328→        print(f\"[RATE LIMIT] Failed to parse reset time: {e}\")\n   329→        return None\n   330→\n   331→\n   332→def check_rate_limit(output: str) -> tuple[bool, Optional[datetime]]:\n   333→    \"\"\"Check if output contains a rate limit message.\n   334→\n   335→    Returns (is_rate_limited, reset_time).\n   336→    reset_time is None if rate limited but couldn't parse the time.\n   337→    \"\"\"\n   338→    # Check for the rate limit pattern\n   339→    if not re.search(r\"(?:You've hit your limit|Usage limit reached)\", output, re.IGNORECASE):\n   340→        return False, None\n   341→\n   342→    reset_time = parse_rate_limit_reset_time(output)\n   343→    return True, reset_time\n   344→\n   345→\n   346→def wait_for_rate_limit_reset(reset_time: Optional[datetime]) -> bool:\n   347→    \"\"\"Sleep until the rate limit resets.\n   348→\n   349→    If reset_time is None, sleeps for RATE_LIMIT_DEFAULT_WAIT_SECONDS.\n   350→    Returns True if the wait completed, False if interrupted by user.\n   351→    \"\"\"\n   352→    if reset_time:\n   353→        now = datetime.now(reset_time.tzinfo)\n   354→        wait_seconds = (reset_time - now).total_seconds()\n   355→        if wait_seconds <= 0:\n   356→            print(\"[RATE LIMIT] Reset time already passed, continuing immediately\")\n   357→            return True\n   358→        # Add a small buffer to ensure we're past the reset\n   359→        wait_seconds += 30\n   360→        reset_str = reset_time.strftime(\"%Y-%m-%d %H:%M %Z\")\n   361→        print(f\"\\n[RATE LIMIT] API rate limit hit. Resets at: {reset_str}\")\n   362→        print(f\"[RATE LIMIT] Sleeping for {wait_seconds:.0f}s ({wait_seconds / 60:.1f} minutes)\")\n   363→    else:\n   364→        wait_seconds = RATE_LIMIT_DEFAULT_WAIT_SECONDS\n   365→        print(f\"\\n[RATE LIMIT] API rate limit hit. Could not parse reset time.\")\n   366→        print(f\"[RATE LIMIT] Sleeping for default {wait_seconds}s ({wait_seconds / 60:.0f} minutes)\")\n   367→\n   368→    print(\"[RATE LIMIT] Press Ctrl+C to abort\")\n   369→    try:\n   370→        time.sleep(wait_seconds)\n   371→        print(\"[RATE LIMIT] Wait complete, resuming...\")\n   372→        return True\n   373→    except KeyboardInterrupt:\n   374→        print(\"\\n[RATE LIMIT] Aborted by user\")\n   375→        return False\n   376→\n   377→\n   378→def load_plan(plan_path: str) -> dict:\n   379→    \"\"\"Load the YAML plan file.\"\"\"\n   380→    with open(plan_path, \"r\") as f:\n   381→        return yaml.safe_load(f)\n   382→\n   383→\n   384→def save_plan(plan_path: str, plan: dict, commit: bool = False, commit_message: str = \"\") -> None:\n   385→    \"\"\"Save the YAML plan file and optionally commit it.\"\"\"\n   386→    with open(plan_path, \"w\") as f:\n   387→        yaml.dump(plan, f, default_flow_style=False, sort_keys=False, allow_unicode=True)\n   388→\n   389→    if commit:\n   390→        try:\n   391→            subprocess.run(\n   392→                [\"git\", \"add\", plan_path],\n   393→                capture_output=True,\n   394→                check=True\n   395→            )\n   396→            msg = commit_message or f\"Update plan: {datetime.now().isoformat()}\"\n   397→            subprocess.run(\n   398→                [\"git\", \"commit\", \"-m\", msg],\n   399→                capture_output=True,\n   400→                check=True\n   401→            )\n   402→            print(f\"[Committed plan changes: {msg}]\")\n   403→        except subprocess.CalledProcessError as e:\n   404→            print(f\"[Warning: Failed to commit plan changes: {e}]\")\n   405→\n   406→\n   407→def read_status_file() -> Optional[dict]:\n   408→    \"\"\"Read the status file written by Claude after task completion.\"\"\"\n   409→    if not os.path.exists(STATUS_FILE_PATH):\n   410→        return None\n   411→    try:\n   412→        with open(STATUS_FILE_PATH, \"r\") as f:\n   413→            return json.load(f)\n   414→    except (json.JSONDecodeError, IOError):\n   415→        return None\n   416→\n   417→\n   418→def clear_status_file() -> None:\n   419→    \"\"\"Clear the status file before running a task.\"\"\"\n   420→    if os.path.exists(STATUS_FILE_PATH):\n   421→        os.remove(STATUS_FILE_PATH)\n   422→\n   423→\n   424→def find_next_task(plan: dict) -> Optional[tuple[dict, dict]]:\n   425→    \"\"\"Find the next pending task to execute. Returns (section, task) or None.\"\"\"\n   426→    sections = plan.get(\"sections\", [])\n   427→    verbose_log(f\"Scanning {len(sections)} sections for next task\", \"FIND\")\n   428→\n   429→    for section in sections:\n   430→        section_id = section.get(\"id\", \"unknown\")\n   431→        section_status = section.get(\"status\", \"pending\")\n   432→        verbose_log(f\"  Section '{section_id}': status={section_status}\", \"FIND\")\n   433→\n   434→        if section_status == \"completed\":\n   435→            verbose_log(f\"    Skipping completed section\", \"FIND\")\n   436→            continue\n   437→\n   438→        tasks = section.get(\"tasks\", [])\n   439→        verbose_log(f\"    Checking {len(tasks)} tasks in section\", \"FIND\")\n   440→\n   441→        for task in tasks:\n   442→            task_id = task.get(\"id\", \"unknown\")\n   443→            task_status = task.get(\"status\", \"pending\")\n   444→            task_depends = task.get(\"depends_on\", [])\n   445→            verbose_log(f\"      Task '{task_id}': status={task_status}, depends_on={task_depends}\", \"FIND\")\n   446→\n   447→            if task_status == \"pending\":\n   448→                # Check dependencies\n   449→                if task_depends:\n   450→                    deps_satisfied = check_dependencies_satisfied(plan, task_depends)\n   451→                    verbose_log(f\"      Dependencies satisfied: {deps_satisfied}\", \"FIND\")\n   452→                    if not deps_satisfied:\n   453→                        verbose_log(f\"      Skipping - dependencies not met\", \"FIND\")\n   454→                        continue\n   455→\n   456→                verbose_log(f\"      SELECTED: Task '{task_id}' is pending and ready\", \"FIND\")\n   457→                return (section, task)\n   458→            elif task_status == \"in_progress\":\n   459→                verbose_log(f\"      SELECTED: Task '{task_id}' is in_progress (resuming)\", \"FIND\")\n   460→                # Resume in-progress task\n   461→                return (section, task)\n   462→            else:\n   463→                verbose_log(f\"      Skipping task with status '{task_status}'\", \"FIND\")\n   464→\n   465→    verbose_log(\"No pending or in_progress tasks found\", \"FIND\")\n   466→    return None\n   467→\n   468→\n   469→def check_dependencies_satisfied(plan: dict, depends_on: list) -> bool:\n   470→    \"\"\"Check if all dependencies for a task are completed.\"\"\"\n   471→    for dep_id in depends_on:\n   472→        result = find_task_by_id(plan, dep_id)\n   473→        if not result:\n   474→            verbose_log(f\"        Dependency '{dep_id}' not found in plan\", \"DEPS\")\n   475→            return False\n   476→        _, dep_task = result\n   477→        dep_status = dep_task.get(\"status\", \"pending\")\n   478→        verbose_log(f\"        Dependency '{dep_id}' status: {dep_status}\", \"DEPS\")\n   479→        if dep_status != \"completed\":\n   480→            return False\n   481→    return True\n   482→\n   483→\n   484→def find_task_by_id(plan: dict, task_id: str) -> Optional[tuple[dict, dict]]:\n   485→    \"\"\"Find a specific task by ID. Returns (section, task) or None.\"\"\"\n   486→    for section in plan.get(\"sections\", []):\n   487→        for task in section.get(\"tasks\", []):\n   488→            if task.get(\"id\") == task_id:\n   489→                return (section, task)\n   490→    return None\n   491→\n   492→\n   493→def extract_files_from_description(description: str) -> set[str]:\n   494→    \"\"\"Extract file paths mentioned in a task description.\n   495→\n   496→    Looks for patterns like:\n   497→    - Files: src/path/to/file.tsx\n   498→    - New: src/path/to/newfile.ts\n   499→    - src/components/Something.tsx\n   500→    - Explicit paths ending in common extensions\n   501→\n   502→    Returns a set of normalized file paths.\n   503→    \"\"\"\n   504→    import re\n   505→\n   506→    files: set[str] = set()\n   507→\n   508→    # Pattern for explicit \"Files:\" or \"New:\" declarations\n   509→    file_patterns = [\n   510→        r'Files?:\\s*([^\\n]+)',\n   511→        r'New:\\s*([^\\n]+)',\n   512→    ]\n   513→\n   514→    for pattern in file_patterns:\n   515→        for match in re.finditer(pattern, description, re.IGNORECASE):\n   516→            # Split by common separators and clean each path\n   517→            paths = re.split(r'[,;]|\\s+', match.group(1))\n   518→            for path in paths:\n   519→                path = path.strip()\n   520→                if path and '/' in path and not path.startswith('#'):\n   521→                    # Remove trailing punctuation\n   522→                    path = re.sub(r'[,;:]+$', '', path)\n   523→                    files.add(path)\n   524→\n   525→    # Pattern for standalone file paths (src/..., test/..., etc.)\n   526→    path_pattern = r'\\b((?:src|test|lib|app|components|hooks|pages|api)/[\\w\\-./]+\\.(?:tsx?|jsx?|md|json|yaml|css|scss))\\b'\n   527→    for match in re.finditer(path_pattern, description):\n   528→        files.add(match.group(1))\n   529→\n   530→    return files\n   531→\n   532→\n   533→def check_parallel_task_conflicts(tasks: list[tuple[dict, dict, str]]) -> tuple[bool, str]:\n   534→    \"\"\"Check if parallel tasks have any file or resource conflicts.\n   535→\n   536→    Returns (has_conflict, conflict_message) tuple.\n   537→    \"\"\"\n   538→    task_files: dict[str, set[str]] = {}\n   539→    task_resources: dict[str, set[str]] = {}\n   540→\n   541→    for section, task, group_name in tasks:\n   542→        task_id = task.get(\"id\", \"unknown\")\n   543→        description = task.get(\"description\", \"\")\n   544→\n   545→        # Extract files from description\n   546→        files = extract_files_from_description(description)\n   547→        task_files[task_id] = files\n   548→\n   549→        # Get exclusive resources declared by task\n   550→        resources = set(task.get(\"exclusive_resources\", []))\n   551→        task_resources[task_id] = resources\n   552→\n   553→    # Check for file overlaps between tasks\n   554→    task_ids = list(task_files.keys())\n   555→    for i in range(len(task_ids)):\n   556→        for j in range(i + 1, len(task_ids)):\n   557→            id1, id2 = task_ids[i], task_ids[j]\n   558→            files1, files2 = task_files[id1], task_files[id2]\n   559→\n   560→            # Check direct file conflicts\n   561→            overlap = files1 & files2\n   562→            if overlap:\n   563→                return (True, f\"Tasks {id1} and {id2} both modify: {', '.join(sorted(overlap))}\")\n   564→\n   565→            # Check directory-level conflicts (if one task modifies a parent directory)\n   566→            for f1 in files1:\n   567→                for f2 in files2:\n   568→                    if f1.startswith(f2.rsplit('/', 1)[0] + '/') or f2.startswith(f1.rsplit('/', 1)[0] + '/'):\n   569→                        # Files in same directory - only warn if same immediate parent\n   570→                        dir1 = f1.rsplit('/', 1)[0] if '/' in f1 else ''\n   571→                        dir2 = f2.rsplit('/', 1)[0] if '/' in f2 else ''\n   572→                        if dir1 == dir2:\n   573→                            verbose_log(f\"Warning: Tasks {id1} and {id2} modify files in same directory: {dir1}\", \"CONFLICT\")\n   574→\n   575→            # Check exclusive resource conflicts\n   576→            res1, res2 = task_resources[id1], task_resources[id2]\n   577→            resource_overlap = res1 & res2\n   578→            if resource_overlap:\n   579→                return (True, f\"Tasks {id1} and {id2} both require exclusive resource: {', '.join(sorted(resource_overlap))}\")\n   580→\n   581→    return (False, \"\")\n   582→\n   583→\n   584→def find_parallel_tasks(plan: dict) -> list[tuple[dict, dict, str]]:\n   585→    \"\"\"Find all tasks ready to run in parallel with the same parallel_group.\n   586→\n   587→    Returns list of (section, task, parallel_group) tuples for tasks that:\n   588→    - Have status 'pending'\n   589→    - Have all dependencies satisfied\n   590→    - Share the same parallel_group value\n   591→    - Do NOT have file or resource conflicts with each other\n   592→\n   593→    Only returns tasks from the first parallel_group found with ready tasks.\n   594→    \"\"\"\n   595→    parallel_groups: dict[str, list[tuple[dict, dict]]] = {}\n   596→\n   597→    for section in plan.get(\"sections\", []):\n   598→        if section.get(\"status\") == \"completed\":\n   599→            continue\n   600→\n   601→        for task in section.get(\"tasks\", []):\n   602→            if task.get(\"status\") != \"pending\":\n   603→                continue\n   604→\n   605→            parallel_group = task.get(\"parallel_group\")\n   606→            if not parallel_group:\n   607→                continue\n   608→\n   609→            # Check dependencies\n   610→            depends_on = task.get(\"depends_on\", [])\n   611→            if depends_on and not check_dependencies_satisfied(plan, depends_on):\n   612→                continue\n   613→\n   614→            if parallel_group not in parallel_groups:\n   615→                parallel_groups[parallel_group] = []\n   616→            parallel_groups[parallel_group].append((section, task))\n   617→\n   618→    # Return tasks from the first parallel group with ready tasks (and no conflicts)\n   619→    for group_name, tasks in parallel_groups.items():\n   620→        if len(tasks) >= 2:  # Only parallelize if 2+ tasks ready\n   621→            candidate_tasks = [(s, t, group_name) for s, t in tasks]\n   622→\n   623→            # Check for conflicts\n   624→            has_conflict, conflict_msg = check_parallel_task_conflicts(candidate_tasks)\n   625→            if has_conflict:\n   626→                print(f\"[CONFLICT] Cannot parallelize group '{group_name}': {conflict_msg}\")\n   627→                print(f\"[CONFLICT] Falling back to sequential execution for conflicting tasks\")\n   628→                verbose_log(f\"Parallel group '{group_name}' has conflicts - executing sequentially\", \"PARALLEL\")\n   629→                # Return empty to fall back to sequential execution\n   630→                return []\n   631→\n   632→            return candidate_tasks\n   633→\n   634→    return []\n   635→\n   636→\n   637→# =============================================================================\n   638→# WORKTREE MANAGEMENT\n   639→# =============================================================================\n   640→\n   641→def get_worktree_path(plan_name: str, task_id: str) -> Path:\n   642→    \"\"\"Get the path for a task's worktree.\"\"\"\n   643→    safe_plan_name = plan_name.replace(\" \", \"-\").lower()[:30]\n   644→    safe_task_id = task_id.replace(\".\", \"-\")\n   645→    return Path(WORKTREE_BASE_DIR) / f\"{safe_plan_name}-{safe_task_id}\"\n   646→\n   647→\n   648→def create_worktree(plan_name: str, task_id: str) -> Optional[Path]:\n   649→    \"\"\"Create a git worktree for a task.\n   650→\n   651→    Returns the worktree path if successful, None if failed.\n   652→    \"\"\"\n   653→    worktree_path = get_worktree_path(plan_name, task_id)\n   654→    branch_name = f\"parallel/{task_id.replace('.', '-')}\"\n   655→\n   656→    # Ensure base directory exists\n   657→    Path(WORKTREE_BASE_DIR).mkdir(parents=True, exist_ok=True)\n   658→\n   659→    # Clean up if worktree already exists\n   660→    if worktree_path.exists():\n   661→        cleanup_worktree(worktree_path)\n   662→\n   663→    # Delete stale branch if it exists (from previous failed run)\n   664→    subprocess.run(\n   665→        [\"git\", \"branch\", \"-D\", branch_name],\n   666→        capture_output=True,\n   667→        text=True,\n   668→        check=False  # Don't fail if branch doesn't exist\n   669→    )\n   670→\n   671→    # Prune any stale worktree references\n   672→    subprocess.run(\n   673→        [\"git\", \"worktree\", \"prune\"],\n   674→        capture_output=True,\n   675→        text=True,\n   676→        check=False\n   677→    )\n   678→\n   679→    try:\n   680→        # Get current branch\n   681→        result = subprocess.run(\n   682→            [\"git\", \"branch\", \"--show-current\"],\n   683→            capture_output=True,\n   684→            text=True,\n   685→            check=True\n   686→        )\n   687→        current_branch = result.stdout.strip()\n   688→\n   689→        # Create worktree with new branch from current HEAD\n   690→        subprocess.run(\n   691→            [\"git\", \"worktree\", \"add\", \"-b\", branch_name, str(worktree_path)],\n   692→            capture_output=True,\n   693→            text=True,\n   694→            check=True\n   695→        )\n   696→\n   697→        verbose_log(f\"Created worktree at {worktree_path} on branch {branch_name}\", \"WORKTREE\")\n   698→\n   699→        # Clear stale task-status.json inherited from main branch to prevent\n   700→        # the orchestrator from reading results from a previous plan's run\n   701→        stale_status = worktree_path / \".claude\" / \"plans\" / \"task-status.json\"\n   702→        if stale_status.exists():\n   703→            stale_status.unlink()\n   704→            verbose_log(\"Removed stale task-status.json from worktree\", \"WORKTREE\")\n   705→\n   706→        return worktree_path\n   707→\n   708→    except subprocess.CalledProcessError as e:\n   709→        print(f\"[ERROR] Failed to create worktree: {e.stderr}\")\n   710→        return None\n   711→\n   712→\n   713→def cleanup_worktree(worktree_path: Path) -> bool:\n   714→    \"\"\"Remove a worktree and its branch.\n   715→\n   716→    Returns True if cleanup was successful.\n   717→    \"\"\"\n   718→    try:\n   719→        # Remove the worktree\n   720→        subprocess.run(\n   721→            [\"git\", \"worktree\", \"remove\", str(worktree_path), \"--force\"],\n   722→            capture_output=True,\n   723→            text=True,\n   724→            check=False  # Don't fail if already removed\n   725→        )\n   726→\n   727→        # Prune any stale worktree references\n   728→        subprocess.run(\n   729→            [\"git\", \"worktree\", \"prune\"],\n   730→            capture_output=True,\n   731→            text=True,\n   732→            check=False\n   733→        )\n   734→\n   735→        verbose_log(f\"Cleaned up worktree at {worktree_path}\", \"WORKTREE\")\n   736→        return True\n   737→\n   738→    except Exception as e:\n   739→        print(f\"[WARNING] Failed to cleanup worktree {worktree_path}: {e}\")\n   740→        return False\n   741→\n   742→\n   743→def cleanup_stale_claims(max_age_minutes: int = 60) -> int:\n   744→    \"\"\"Remove stale claims from agent-claims.json.\n   745→\n   746→    A claim is stale if:\n   747→    - Its claimed_at timestamp is older than max_age_minutes\n   748→    - OR no corresponding subagent status file exists with recent heartbeat\n   749→\n   750→    Returns the number of claims removed.\n   751→    \"\"\"\n   752→    claims_path = Path(\".claude/agent-claims.json\")\n   753→    if not claims_path.exists():\n   754→        return 0\n   755→\n   756→    try:\n   757→        with open(claims_path, \"r\") as f:\n   758→            data = json.load(f)\n   759→    except (json.JSONDecodeError, IOError):\n   760→        return 0\n   761→\n   762→    claims = data.get(\"claims\", [])\n   763→    if not claims:\n   764→        return 0\n   765→\n   766→    now = datetime.now()\n   767→    max_age = max_age_minutes * 60  # Convert to seconds\n   768→    active_claims = []\n   769→    removed_count = 0\n   770→\n   771→    for claim in claims:\n   772→        claimed_at_str = claim.get(\"claimed_at\", \"\")\n   773→        agent_id = claim.get(\"agent\", \"\")\n   774→\n   775→        # Parse claimed_at timestamp\n   776→        try:\n   777→            claimed_at = datetime.fromisoformat(claimed_at_str.replace(\"Z\", \"+00:00\"))\n   778→            # Make naive for comparison\n   779→            if claimed_at.tzinfo:\n   780→                claimed_at = claimed_at.replace(tzinfo=None)\n   781→            age_seconds = (now - claimed_at).total_seconds()\n   782→        except (ValueError, TypeError):\n   783→            age_seconds = float('inf')  # Invalid timestamp = stale\n   784→\n   785→        # Check if claim is stale by age\n   786→        if age_seconds > max_age:\n   787→            verbose_log(f\"Removing stale claim from {agent_id} (age: {age_seconds/60:.1f} min)\", \"CLEANUP\")\n   788→            removed_count += 1\n   789→            continue\n   790→\n   791→        # Check subagent status file for heartbeat\n   792→        status_path = Path(f\".claude/subagent-status/{agent_id}.json\")\n   793→        if status_path.exists():\n   794→            try:\n   795→                with open(status_path, \"r\") as f:\n   796→                    status = json.load(f)\n   797→                # If status is completed/failed, claim should be released\n   798→                if status.get(\"status\") in [\"completed\", \"failed\"]:\n   799→                    verbose_log(f\"Removing claim from {agent_id} (status: {status.get('status')})\", \"CLEANUP\")\n   800→                    removed_count += 1\n   801→                    continue\n   802→            except (json.JSONDecodeError, IOError):\n   803→                pass\n   804→\n   805→        # Keep this claim\n   806→        active_claims.append(claim)\n   807→\n   808→    # Write back if changes were made\n   809→    if removed_count > 0:\n   810→        data[\"claims\"] = active_claims\n   811→        with open(claims_path, \"w\") as f:\n   812→            json.dump(data, f, indent=2)\n   813→        verbose_log(f\"Cleaned up {removed_count} stale claims\", \"CLEANUP\")\n   814→\n   815→    return removed_count\n   816→\n   817→\n   818→def copy_worktree_artifacts(worktree_path: Path, task_id: str) -> tuple[bool, str, list[str]]:\n   819→    \"\"\"Copy changed files from a worktree into the main working directory.\n   820→\n   821→    Instead of using git merge (which fails when multiple parallel branches all\n   822→    modify the YAML plan file), this function:\n   823→    1. Diffs the worktree branch against the fork point to find changed files\n   824→    2. Copies added/modified files from the worktree into main\n   825→    3. Removes deleted files from main\n   826→    4. Skips coordination files (.claude/plans/) that the orchestrator manages\n   827→\n   828→    Returns (success, message, files_copied) tuple.\n   829→    \"\"\"\n   830→    branch_name = f\"parallel/{task_id.replace('.', '-')}\"\n   831→    # Coordination paths that the orchestrator manages -- never copy from worktrees\n   832→    SKIP_PREFIXES = (\".claude/plans/\", \".claude/subagent-status/\", \".claude/agent-claims\")\n   833→\n   834→    try:\n   835→        # Find the fork point (common ancestor of main and branch)\n   836→        fork_result = subprocess.run(\n   837→            [\"git\", \"merge-base\", \"HEAD\", branch_name],\n   838→            capture_output=True, text=True, check=True\n   839→        )\n   840→        fork_point = fork_result.stdout.strip()\n   841→\n   842→        # Get list of changed files between fork point and branch tip\n   843→        diff_result = subprocess.run(\n   844→            [\"git\", \"diff\", \"--name-status\", fork_point, branch_name],\n   845→            capture_output=True, text=True, check=True\n   846→        )\n   847→\n   848→        if not diff_result.stdout.strip():\n   849→            verbose_log(f\"No file changes in {branch_name}\", \"MERGE\")\n   850→            return (True, \"No changes to copy\", [])\n   851→\n   852→        files_copied: list[str] = []\n   853→        files_deleted: list[str] = []\n   854→        files_skipped: list[str] = []\n   855→\n   856→        for line in diff_result.stdout.strip().splitlines():\n   857→            parts = line.split(\"\\t\")\n   858→            if len(parts) < 2:\n   859→                continue\n   860→            status = parts[0][0]  # A, M, D, R, C (first char)\n   861→            file_path = parts[-1]  # Last element handles renames\n   862→\n   863→            # Skip orchestrator coordination files\n   864→            if any(file_path.startswith(prefix) for prefix in SKIP_PREFIXES):\n   865→                files_skipped.append(file_path)\n   866→                verbose_log(f\"  Skipping coordination file: {file_path}\", \"MERGE\")\n   867→                continue\n   868→\n   869→            if status in (\"A\", \"M\", \"C\"):\n   870→                # Added, Modified, or Copied -- copy from worktree to main\n   871→                src = worktree_path / file_path\n   872→                dst = Path(file_path)\n   873→                if src.exists():\n   874→                    dst.parent.mkdir(parents=True, exist_ok=True)\n   875→                    shutil.copy2(str(src), str(dst))\n   876→                    files_copied.append(file_path)\n   877→                    verbose_log(f\"  Copied: {file_path}\", \"MERGE\")\n   878→                else:\n   879→                    verbose_log(f\"  WARNING: Source missing in worktree: {file_path}\", \"MERGE\")\n   880→\n   881→            elif status == \"D\":\n   882→                # Deleted -- remove from main if it exists\n   883→                dst = Path(file_path)\n   884→                if dst.exists():\n   885→                    dst.unlink()\n   886→                    files_deleted.append(file_path)\n   887→                    verbose_log(f\"  Deleted: {file_path}\", \"MERGE\")\n   888→\n   889→            elif status == \"R\":\n   890→                # Renamed -- old path in parts[1], new path in parts[2]\n   891→                if len(parts) >= 3:\n   892→                    old_path = parts[1]\n   893→                    new_path = parts[2]\n   894→                    src = worktree_path / new_path\n   895→                    if src.exists():\n   896→                        Path(new_path).parent.mkdir(parents=True, exist_ok=True)\n   897→                        shutil.copy2(str(src), str(new_path))\n   898→                        files_copied.append(new_path)\n   899→                        # Remove old file if it still exists in main\n   900→                        old_dst = Path(old_path)\n   901→                        if old_dst.exists():\n   902→                            old_dst.unlink()\n   903→                            files_deleted.append(old_path)\n   904→                        verbose_log(f\"  Renamed: {old_path} -> {new_path}\", \"MERGE\")\n   905→\n   906→        all_changes = files_copied + files_deleted\n   907→        summary_parts = []\n   908→        if files_copied:\n   909→            summary_parts.append(f\"{len(files_copied)} copied\")\n   910→        if files_deleted:\n   911→            summary_parts.append(f\"{len(files_deleted)} deleted\")\n   912→        if files_skipped:\n   913→            summary_parts.append(f\"{len(files_skipped)} skipped\")\n   914→        summary = \", \".join(summary_parts) or \"no file changes\"\n   915→\n   916→        verbose_log(f\"Artifacts from {branch_name}: {summary}\", \"MERGE\")\n   917→\n   918→        # Delete the branch after successful copy\n   919→        subprocess.run(\n   920→            [\"git\", \"branch\", \"-D\", branch_name],\n   921→            capture_output=True, text=True, check=False\n   922→        )\n   923→\n   924→        return (True, f\"Copied from {branch_name}: {summary}\", all_changes)\n   925→\n   926→    except subprocess.CalledProcessError as e:\n   927→        error_msg = e.stderr or e.stdout or str(e)\n   928→        return (False, f\"Failed to copy artifacts: {error_msg}\", [])\n   929→\n   930→\n   931→def run_parallel_task(\n   932→    plan: dict,\n   933→    section: dict,\n   934→    task: dict,\n   935→    plan_path: str,\n   936→    plan_name: str,\n   937→    parallel_group: str,\n   938→    sibling_task_ids: list[str],\n   939→    dry_run: bool = False\n   940→) -> tuple[str, TaskResult]:\n   941→    \"\"\"Run a single task in a worktree for parallel execution.\n   942→\n   943→    Returns (task_id, TaskResult) tuple.\n   944→    \"\"\"\n   945→    task_id = task.get(\"id\")\n   946→    verbose_log(f\"[PARALLEL] Starting task {task_id}\", \"PARALLEL\")\n   947→\n   948→    if dry_run:\n   949→        print(f\"[DRY RUN] Would execute task {task_id} in worktree\")\n   950→        return (task_id, TaskResult(success=True, message=\"Dry run\", duration_seconds=0))\n   951→\n   952→    # Create worktree\n   953→    worktree_path = create_worktree(plan_name, task_id)\n   954→    if not worktree_path:\n   955→        return (task_id, TaskResult(\n   956→            success=False,\n   957→            message=f\"Failed to create worktree for task {task_id}\",\n   958→            duration_seconds=0\n   959→        ))\n   960→\n   961→    start_time = time.time()\n   962→\n   963→    # Build subagent context\n   964→    agent_id = f\"subagent-{task_id.replace('.', '-')}\"\n   965→    subagent_context = {\n   966→        \"agent_id\": agent_id,\n   967→        \"worktree_path\": str(worktree_path),\n   968→        \"parallel_group\": parallel_group,\n   969→        \"sibling_tasks\": [tid for tid in sibling_task_ids if tid != task_id]\n   970→    }\n   971→\n   972→    try:\n   973→        # Build prompt with subagent context\n   974→        prompt = build_claude_prompt(plan, section, task, plan_path, subagent_context)\n   975→\n   976→        # Run Claude in the worktree directory\n   977→        cmd = [\n   978→            *CLAUDE_CMD,\n   979→            \"--dangerously-skip-permissions\",\n   980→            \"--print\",\n   981→            prompt\n   982→        ]\n   983→\n   984→        verbose_log(f\"[PARALLEL] Running Claude for task {task_id} in {worktree_path}\", \"PARALLEL\")\n   985→\n   986→        process = subprocess.Popen(\n   987→            cmd,\n   988→            stdout=subprocess.PIPE,\n   989→            stderr=subprocess.PIPE,\n   990→            text=True,\n   991→            cwd=str(worktree_path),\n   992→            env=build_child_env()\n   993→        )\n   994→\n   995→        stdout, stderr = process.communicate(timeout=CLAUDE_TIMEOUT_SECONDS)\n   996→        duration = time.time() - start_time\n   997→\n   998→        if process.returncode != 0:\n   999→            # Check for rate limit in parallel task output\n  1000→            combined_output = (stdout or \"\") + \"\\n\" + (stderr or \"\")\n  1001→            is_rate_limited, reset_time = check_rate_limit(combined_output)\n  1002→\n  1003→            if is_rate_limited:\n  1004→                return (task_id, TaskResult(\n  1005→                    success=False,\n  1006→                    message=\"API rate limit reached\",\n  1007→                    duration_seconds=duration,\n  1008→                    rate_limited=True,\n  1009→                    rate_limit_reset_time=reset_time\n  1010→                ))\n  1011→\n  1012→            return (task_id, TaskResult(\n  1013→                success=False,\n  1014→                message=f\"Claude exited with code {process.returncode}: {stderr[:500]}\",\n  1015→                duration_seconds=duration\n  1016→            ))\n  1017→\n  1018→        # Check status file in worktree\n  1019→        worktree_status_file = worktree_path / \".claude\" / \"plans\" / \"task-status.json\"\n  1020→        status = None\n  1021→        if worktree_status_file.exists():\n  1022→            try:\n  1023→                with open(worktree_status_file, \"r\") as f:\n  1024→                    status = json.load(f)\n  1025→            except (json.JSONDecodeError, IOError):\n  1026→                pass\n  1027→\n  1028→        if status and status.get(\"status\") == \"completed\":\n  1029→            # Commit changes in worktree before returning\n  1030→            try:\n  1031→                subprocess.run(\n  1032→                    [\"git\", \"add\", \"-A\"],\n  1033→                    cwd=str(worktree_path),\n  1034→                    capture_output=True,\n  1035→                    check=True\n  1036→                )\n  1037→                subprocess.run(\n  1038→                    [\"git\", \"commit\", \"-m\", f\"plan: Task {task_id} completed (parallel)\"],\n  1039→                    cwd=str(worktree_path),\n  1040→                    capture_output=True,\n  1041→                    check=False  # May have nothing to commit\n  1042→                )\n  1043→            except subprocess.CalledProcessError:\n  1044→                pass  # No changes to commit is OK\n  1045→\n  1046→            return (task_id, TaskResult(\n  1047→                success=True,\n  1048→                message=status.get(\"message\", \"Task completed\"),\n  1049→                duration_seconds=duration,\n  1050→                plan_modified=status.get(\"plan_modified\", False)\n  1051→            ))\n  1052→        elif status and status.get(\"status\") == \"failed\":\n  1053→            return (task_id, TaskResult(\n  1054→                success=False,\n  1055→                message=status.get(\"message\", \"Task failed\"),\n  1056→                duration_seconds=duration\n  1057→            ))\n  1058→        else:\n  1059→            return (task_id, TaskResult(\n  1060→                success=False,\n  1061→                message=\"No status file written by Claude\",\n  1062→                duration_seconds=duration\n  1063→            ))\n  1064→\n  1065→    except subprocess.TimeoutExpired:\n  1066→        return (task_id, TaskResult(\n  1067→            success=False,\n  1068→            message=f\"Task timed out after {CLAUDE_TIMEOUT_SECONDS} seconds\",\n  1069→            duration_seconds=CLAUDE_TIMEOUT_SECONDS\n  1070→        ))\n  1071→    except Exception as e:\n  1072→        return (task_id, TaskResult(\n  1073→            success=False,\n  1074→            message=f\"Error running parallel task: {str(e)}\",\n  1075→            duration_seconds=time.time() - start_time\n  1076→        ))\n  1077→\n  1078→\n  1079→def build_claude_prompt(\n  1080→    plan: dict,\n  1081→    section: dict,\n  1082→    task: dict,\n  1083→    plan_path: str,\n  1084→    subagent_context: Optional[dict] = None\n  1085→) -> str:\n  1086→    \"\"\"Build the prompt for Claude to execute a task.\n  1087→\n  1088→    Args:\n  1089→        plan: The full plan dict\n  1090→        section: The section containing the task\n  1091→        task: The task to execute\n  1092→        plan_path: Path to the YAML plan file\n  1093→        subagent_context: Optional dict with subagent info for parallel execution\n  1094→            - agent_id: Unique identifier for this subagent\n  1095→            - worktree_path: Path to this agent's worktree\n  1096→            - parallel_group: Name of the parallel group\n  1097→            - sibling_tasks: List of other task IDs running in parallel\n  1098→    \"\"\"\n  1099→    plan_doc = plan.get(\"meta\", {}).get(\"plan_doc\", \"\")\n  1100→\n  1101→    # Add subagent header if running as parallel worker\n  1102→    subagent_header = \"\"\n  1103→    if subagent_context:\n  1104→        subagent_header = f\"\"\"\n  1105→## SUBAGENT CONTEXT (You are a parallel worker)\n  1106→\n  1107→**SUBAGENT_ID:** {subagent_context['agent_id']}\n  1108→**WORKTREE_PATH:** {subagent_context['worktree_path']}\n  1109→**PARALLEL_GROUP:** {subagent_context['parallel_group']}\n  1110→**SIBLING_TASKS:** {', '.join(subagent_context.get('sibling_tasks', []))}\n  1111→\n  1112→### MANDATORY: Follow the agent-sync protocol\n  1113→\n  1114→You are running in parallel with other agents. Before editing ANY file:\n  1115→\n  1116→1. **Initialize your status file:**\n  1117→   ```bash\n  1118→   mkdir -p .claude/subagent-status\n  1119→   echo '{{\"status\":\"starting\",\"task_id\":\"{task['id']}\",\"heartbeat\":\"'$(date -u +%Y-%m-%dT%H:%M:%SZ)'\"}}' > .claude/subagent-status/{subagent_context['agent_id']}.json\n  1120→   ```\n  1121→\n  1122→2. **Check claims before editing:**\n  1123→   ```bash\n  1124→   cat .claude/agent-claims.json\n  1125→   ```\n  1126→\n  1127→3. **Claim files before editing** - Add your claim to agent-claims.json\n  1128→\n  1129→4. **Update status as you work** - Keep heartbeat current\n  1130→\n  1131→5. **Release claims when done** - Remove from agent-claims.json\n  1132→\n  1133→6. **Write final status:**\n  1134→   ```bash\n  1135→   echo '{{\"status\":\"completed\",\"task_id\":\"{task['id']}\",\"heartbeat\":\"'$(date -u +%Y-%m-%dT%H:%M:%SZ)'\"}}' > .claude/subagent-status/{subagent_context['agent_id']}.json\n  1136→   ```\n  1137→\n  1138→Read `.claude/skills/agent-sync.md` for the full protocol.\n  1139→\n  1140→---\n  1141→\n  1142→\"\"\"\n  1143→\n  1144→    return f\"\"\"{subagent_header}Run task {task['id']} from the implementation plan.\n  1145→\n  1146→## Task Details\n  1147→- **Section:** {section['name']} ({section['id']})\n  1148→- **Task:** {task['name']}\n  1149→- **Description:** {task.get('description', 'No description')}\n  1150→- **Plan Document:** {plan_doc}\n  1151→- **YAML Plan File:** {plan_path}\n  1152→\n  1153→## Instructions\n  1154→1. First, verify the current state - a previous attempt may have failed\n  1155→2. Read the relevant section from the plan document for detailed implementation steps\n  1156→3. Implement the task following the plan's specifications\n  1157→4. Run `pnpm run build` to verify no TypeScript errors\n  1158→5. If you changed middleware, layout files, or auth-related code: run `npx playwright test tests/SMOKE01-critical-paths.spec.ts --reporter=list` to verify critical paths\n  1159→6. Commit your changes with a descriptive message\n  1160→6. Write a status file to `.claude/plans/task-status.json` with this format:\n  1161→   ```json\n  1162→   {{\n  1163→     \"task_id\": \"{task['id']}\",\n  1164→     \"status\": \"completed\",  // or \"failed\"\n  1165→     \"message\": \"Brief description of what was done or what failed\",\n  1166→     \"timestamp\": \"<ISO timestamp>\",\n  1167→     \"plan_modified\": false  // set to true if you modified the YAML plan\n  1168→   }}\n  1169→   ```\n  1170→\n  1171→## Plan Modification (Optional)\n  1172→You MAY modify the YAML plan file ({plan_path}) if it makes sense:\n  1173→- **Split a task** that's too large into smaller subtasks (e.g., 5.2 -> 5.2a, 5.2b)\n  1174→- **Add a task** if you discover something missing from the plan\n  1175→- **Update descriptions** to be more accurate based on what you learned\n  1176→- **Add notes** to tasks with important context\n  1177→- **Skip a task** by setting status to \"skipped\" with a reason if it's no longer needed\n  1178→\n  1179→If you modify the plan, set \"plan_modified\": true in the status file so the orchestrator reloads it.\n  1180→\n  1181→IMPORTANT: You MUST write the status file before finishing. This is how the orchestrator knows the task result.\n  1182→\"\"\"\n  1183→\n  1184→\n  1185→def run_smoke_tests() -> bool:\n  1186→    \"\"\"Run smoke tests to verify critical paths still work after plan completion.\n  1187→\n  1188→    Returns True if smoke tests pass, False otherwise.\n  1189→    Uses PLAYWRIGHT_BASE_URL to target an existing dev server (avoids starting a\n  1190→    competing QA server that would corrupt the shared .next cache).\n  1191→    \"\"\"\n  1192→    print(\"\\n=== Running post-plan smoke tests ===\")\n  1193→    print(\"Verifying critical user paths...\")\n  1194→\n  1195→    # Detect which port has a running server\n  1196→    smoke_port = None\n  1197→    try:\n  1198→        check = subprocess.run(\n  1199→            [\"lsof\", \"-ti\", f\":{DEV_SERVER_PORT}\"],\n  1200→            capture_output=True, text=True, timeout=5\n  1201→        )\n  1202→        if check.stdout.strip():\n  1203→            smoke_port = DEV_SERVER_PORT\n  1204→    except Exception:\n  1205→        pass\n  1206→\n  1207→    if not smoke_port:\n  1208→        print(f\"[SMOKE] No dev server detected on port {DEV_SERVER_PORT} - skipping smoke tests\")\n  1209→        print(\"[SMOKE] Start a server with 'pnpm dev' before running smoke tests\")\n  1210→        return True  # Don't fail the plan if no server is running\n  1211→\n  1212→    print(f\"[SMOKE] Using existing server on port {smoke_port}\")\n  1213→\n  1214→    # Set env to target existing server and skip webServer startup\n  1215→    env = os.environ.copy()\n  1216→    env[\"PLAYWRIGHT_BASE_URL\"] = f\"http://localhost:{smoke_port}\"\n  1217→    env[\"SMOKE_SKIP_WEBSERVER\"] = \"true\"\n  1218→\n  1219→    try:\n  1220→        result = subprocess.run(\n  1221→            [\"npx\", \"playwright\", \"test\", \"tests/SMOKE01-critical-paths.spec.ts\",\n  1222→             \"--reporter=list\", \"--timeout=30000\",\n  1223→             \"--project=chromium\"],\n  1224→            capture_output=True,\n  1225→            text=True,\n  1226→            timeout=180,\n  1227→            cwd=os.getcwd(),\n  1228→            env=env\n  1229→        )\n  1230→\n  1231→        if result.returncode == 0:\n  1232→            print(\"[SMOKE] All smoke tests PASSED\")\n  1233→            return True\n  1234→        else:\n  1235→            print(f\"[SMOKE] Smoke tests FAILED (exit code {result.returncode})\")\n  1236→            # Show relevant output\n  1237→            for line in result.stdout.splitlines()[-20:]:\n  1238→                print(f\"  {line}\")\n  1239→            if result.stderr:\n  1240→                for line in result.stderr.splitlines()[-10:]:\n  1241→                    print(f\"  [stderr] {line}\")\n  1242→            return False\n  1243→    except subprocess.TimeoutExpired:\n  1244→        print(\"[SMOKE] Smoke tests TIMED OUT (180s)\")\n  1245→        return False\n  1246→    except FileNotFoundError:\n  1247→        print(\"[SMOKE] npx/playwright not found - skipping smoke tests\")\n  1248→        return True  # Don't fail the plan if playwright isn't installed\n  1249→\n  1250→\n  1251→class OutputCollector:\n  1252→    \"\"\"Collects output from Claude CLI and tracks stats.\"\"\"\n  1253→\n  1254→    def __init__(self):\n  1255→        self.lines: list[str] = []\n  1256→        self.bytes_received = 0\n  1257→        self.line_count = 0\n  1258→\n  1259→    def add_line(self, line: str) -> None:\n  1260→        self.lines.append(line)\n  1261→        self.bytes_received += len(line.encode('utf-8'))\n  1262→        self.line_count += 1\n  1263→\n  1264→    def get_output(self) -> str:\n  1265→        return ''.join(self.lines)\n  1266→\n  1267→\n  1268→def stream_output(pipe, prefix: str, collector: OutputCollector, show_full: bool) -> None:\n  1269→    \"\"\"Stream output from a subprocess pipe line by line.\"\"\"\n  1270→    try:\n  1271→        for line in iter(pipe.readline, ''):\n  1272→            if line:\n  1273→                collector.add_line(line)\n  1274→                if show_full:\n  1275→                    print(f\"[CLAUDE {prefix}] {line.rstrip()}\", flush=True)\n  1276→    except Exception as e:\n  1277→        if VERBOSE:\n  1278→            verbose_log(f\"Error streaming {prefix}: {e}\", \"ERROR\")\n  1279→\n  1280→\n  1281→def stream_json_output(pipe, collector: OutputCollector) -> None:\n  1282→    \"\"\"Stream output from Claude CLI in stream-json format, showing tool use and text in real-time.\"\"\"\n  1283→    try:\n  1284→        for line in iter(pipe.readline, ''):\n  1285→            if not line:\n  1286→                continue\n  1287→            collector.add_line(line)\n  1288→            try:\n  1289→                event = json.loads(line)\n  1290→            except (json.JSONDecodeError, ValueError):\n  1291→                continue\n  1292→\n  1293→            event_type = event.get(\"type\", \"\")\n  1294→\n  1295→            ts = datetime.now().strftime(\"%H:%M:%S\")\n  1296→\n  1297→            if event_type == \"assistant\":\n  1298→                msg = event.get(\"message\", {})\n  1299→                for block in msg.get(\"content\", []):\n  1300→                    block_type = block.get(\"type\", \"\")\n  1301→                    if block_type == \"text\":\n  1302→                        text = block.get(\"text\", \"\").strip()\n  1303→                        if text:\n  1304→                            # Show first 200 chars of text blocks\n  1305→                            display = text[:200] + (\"...\" if len(text) > 200 else \"\")\n  1306→                            print(f\"  [{ts}] [Claude] {display}\", flush=True)\n  1307→                    elif block_type == \"tool_use\":\n  1308→                        tool_name = block.get(\"name\", \"?\")\n  1309→                        tool_input = block.get(\"input\", {})\n  1310→                        # Show tool name + key info\n  1311→                        if tool_name in (\"Read\", \"Edit\", \"Write\"):\n  1312→                            detail = tool_input.get(\"file_path\", \"\")\n  1313→                        elif tool_name == \"Bash\":\n  1314→                            cmd = tool_input.get(\"command\", \"\")\n  1315→                            detail = cmd[:80] + (\"...\" if len(cmd) > 80 else \"\")\n  1316→                        elif tool_name in (\"Grep\", \"Glob\"):\n  1317→                            detail = tool_input.get(\"pattern\", \"\")\n  1318→                        else:\n  1319→                            detail = \"\"\n  1320→                        print(f\"  [{ts}] [Tool] {tool_name}: {detail}\", flush=True)\n  1321→\n  1322→            elif event_type == \"result\":\n  1323→                cost = event.get(\"total_cost_usd\", 0)\n  1324→                duration = event.get(\"duration_ms\", 0) / 1000\n  1325→                turns = event.get(\"num_turns\", 0)\n  1326→                print(f\"  [{ts}] [Result] {turns} turns, {duration:.1f}s, ${cost:.4f}\", flush=True)\n  1327→\n  1328→    except Exception as e:\n  1329→        if VERBOSE:\n  1330→            verbose_log(f\"Error streaming JSON: {e}\", \"ERROR\")\n  1331→\n  1332→\n  1333→def run_claude_task(prompt: str, dry_run: bool = False) -> TaskResult:\n  1334→    \"\"\"Execute a task using Claude CLI.\"\"\"\n  1335→    if dry_run:\n  1336→        print(f\"[DRY RUN] Would execute:\\n{prompt[:200]}...\")\n  1337→        return TaskResult(success=True, message=\"Dry run\", duration_seconds=0)\n  1338→\n  1339→    start_time = time.time()\n  1340→\n  1341→    verbose_log(\"Building Claude CLI command\", \"EXEC\")\n  1342→    cmd = [\n  1343→        *CLAUDE_CMD,\n  1344→        \"--dangerously-skip-permissions\",\n  1345→        \"--print\",\n  1346→        prompt\n  1347→    ]\n  1348→    # In verbose mode, use stream-json for real-time tool/text streaming\n  1349→    if VERBOSE:\n  1350→        cmd.extend([\"--output-format\", \"stream-json\", \"--verbose\"])\n  1351→    verbose_log(f\"Command: {' '.join(CLAUDE_CMD)} --dangerously-skip-permissions --print <prompt>\", \"EXEC\")\n  1352→    verbose_log(f\"Prompt length: {len(prompt)} chars\", \"EXEC\")\n  1353→    verbose_log(f\"Working directory: {os.getcwd()}\", \"EXEC\")\n  1354→    verbose_log(f\"Timeout: {CLAUDE_TIMEOUT_SECONDS}s\", \"EXEC\")\n  1355→\n  1356→    # Output collectors for both modes\n  1357→    stdout_collector = OutputCollector()\n  1358→    stderr_collector = OutputCollector()\n  1359→\n  1360→    try:\n  1361→        if VERBOSE:\n  1362→            verbose_log(\"Starting Claude CLI process with real-time streaming...\", \"EXEC\")\n  1363→            print(\"=\" * 60, flush=True)\n  1364→            print(\"[CLAUDE OUTPUT START]\", flush=True)\n  1365→            print(\"=\" * 60, flush=True)\n  1366→\n  1367→        process = subprocess.Popen(\n  1368→            cmd,\n  1369→            stdin=subprocess.DEVNULL,\n  1370→            stdout=subprocess.PIPE,\n  1371→            stderr=subprocess.PIPE,\n  1372→            text=True,\n  1373→            cwd=os.getcwd(),\n  1374→            env=build_child_env()\n  1375→        )\n  1376→\n  1377→        # Start threads to stream/collect stdout and stderr\n  1378→        if VERBOSE:\n  1379→            stdout_thread = threading.Thread(\n  1380→                target=stream_json_output,\n  1381→                args=(process.stdout, stdout_collector)\n  1382→            )\n  1383→        else:\n  1384→            stdout_thread = threading.Thread(\n  1385→                target=stream_output,\n  1386→                args=(process.stdout, \"OUT\", stdout_collector, False)\n  1387→            )\n  1388→        stderr_thread = threading.Thread(\n  1389→            target=stream_output,\n  1390→            args=(process.stderr, \"ERR\", stderr_collector, VERBOSE)\n  1391→        )\n  1392→        stdout_thread.start()\n  1393→        stderr_thread.start()\n  1394→\n  1395→        # In regular mode, show progress updates\n  1396→        if not VERBOSE:\n  1397→            print(\"[Claude] Working\", end=\"\", flush=True)\n  1398→            last_bytes = 0\n  1399→            dots = 0\n  1400→            while process.poll() is None:\n  1401→                time.sleep(1)\n  1402→                current_bytes = stdout_collector.bytes_received + stderr_collector.bytes_received\n  1403→                if current_bytes > last_bytes:\n  1404→                    # Show a dot for each 1KB received\n  1405→                    new_kb = (current_bytes - last_bytes) // 1024\n  1406→                    if new_kb > 0:\n  1407→                        print(\".\" * min(new_kb, 5), end=\"\", flush=True)\n  1408→                        dots += min(new_kb, 5)\n  1409→                    elif current_bytes > last_bytes:\n  1410→                        print(\".\", end=\"\", flush=True)\n  1411→                        dots += 1\n  1412→                    last_bytes = current_bytes\n  1413→\n  1414→                # Check timeout\n  1415→                elapsed = time.time() - start_time\n  1416→                if elapsed > CLAUDE_TIMEOUT_SECONDS:\n  1417→                    print(\" [TIMEOUT]\", flush=True)\n  1418→                    process.terminate()\n  1419→                    process.wait(timeout=5)\n  1420→                    raise subprocess.TimeoutExpired(cmd, CLAUDE_TIMEOUT_SECONDS)\n  1421→\n  1422→            print(f\" done ({stdout_collector.line_count} lines, {stdout_collector.bytes_received:,} bytes)\", flush=True)\n  1423→\n  1424→        # Wait for process with timeout (verbose mode)\n  1425→        if VERBOSE:\n  1426→            try:\n  1427→                returncode = process.wait(timeout=CLAUDE_TIMEOUT_SECONDS)\n  1428→            except subprocess.TimeoutExpired:\n  1429→                verbose_log(\"Process timed out, terminating...\", \"EXEC\")\n  1430→                process.terminate()\n  1431→                process.wait(timeout=5)\n  1432→                raise\n  1433→        else:\n  1434→            returncode = process.returncode\n  1435→\n  1436→        # Wait for threads to finish\n  1437→        stdout_thread.join(timeout=5)\n  1438→        stderr_thread.join(timeout=5)\n  1439→\n  1440→        if VERBOSE:\n  1441→            print(\"=\" * 60, flush=True)\n  1442→            print(\"[CLAUDE OUTPUT END]\", flush=True)\n  1443→            print(\"=\" * 60, flush=True)\n  1444→            verbose_log(f\"Process completed with return code: {returncode}\", \"EXEC\")\n  1445→\n  1446→        duration = time.time() - start_time\n  1447→\n  1448→        # Save output to log file for debugging\n  1449→        log_dir = Path(\".claude/plans/logs\")\n  1450→        log_dir.mkdir(parents=True, exist_ok=True)\n  1451→        log_file = log_dir / f\"task-{datetime.now().strftime('%Y%m%d-%H%M%S')}.log\"\n  1452→        with open(log_file, \"w\") as f:\n  1453→            f.write(f\"=== Claude Task Output ===\\n\")\n  1454→            f.write(f\"Timestamp: {datetime.now().isoformat()}\\n\")\n  1455→            f.write(f\"Duration: {duration:.1f}s\\n\")\n  1456→            f.write(f\"Return code: {returncode}\\n\")\n  1457→            f.write(f\"Stdout lines: {stdout_collector.line_count}\\n\")\n  1458→            f.write(f\"Stderr lines: {stderr_collector.line_count}\\n\")\n  1459→            f.write(f\"\\n=== STDOUT ===\\n\")\n  1460→            f.write(stdout_collector.get_output())\n  1461→            f.write(f\"\\n=== STDERR ===\\n\")\n  1462→            f.write(stderr_collector.get_output())\n  1463→\n  1464→        print(f\"[Log saved to: {log_file}]\")\n  1465→\n  1466→        if VERBOSE:\n  1467→            verbose_log(f\"Duration: {duration:.1f}s\", \"EXEC\")\n  1468→\n  1469→        if returncode != 0:\n  1470→            # Combine stdout and stderr to check for rate limit messages\n  1471→            combined_output = stdout_collector.get_output() + \"\\n\" + stderr_collector.get_output()\n  1472→            is_rate_limited, reset_time = check_rate_limit(combined_output)\n  1473→\n  1474→            if is_rate_limited:\n  1475→                return TaskResult(\n  1476→                    success=False,\n  1477→                    message=\"API rate limit reached\",\n  1478→                    duration_seconds=duration,\n  1479→                    rate_limited=True,\n  1480→                    rate_limit_reset_time=reset_time\n  1481→                )\n  1482→\n  1483→            error_msg = stderr_collector.get_output()[:500] if stderr_collector.bytes_received > 0 else \"Unknown error\"\n  1484→            return TaskResult(\n  1485→                success=False,\n  1486→                message=f\"Claude exited with code {returncode}: {error_msg}\",\n  1487→                duration_seconds=duration\n  1488→            )\n  1489→\n  1490→        # Check the status file for task result\n  1491→        verbose_log(f\"Checking status file: {STATUS_FILE_PATH}\", \"STATUS\")\n  1492→        status = read_status_file()\n  1493→\n  1494→        if status:\n  1495→            verbose_log(f\"Status file contents: {json.dumps(status, indent=2)}\", \"STATUS\")\n  1496→        else:\n  1497→            verbose_log(\"No status file found or failed to parse\", \"STATUS\")\n  1498→\n  1499→        plan_modified = status.get(\"plan_modified\", False) if status else False\n  1500→\n  1501→        if status and status.get(\"status\") == \"completed\":\n  1502→            verbose_log(\"Task status: COMPLETED\", \"STATUS\")\n  1503→            return TaskResult(\n  1504→                success=True,\n  1505→                message=status.get(\"message\", \"Task completed\"),\n  1506→                duration_seconds=duration,\n  1507→                plan_modified=plan_modified\n  1508→            )\n  1509→        elif status and status.get(\"status\") == \"failed\":\n  1510→            verbose_log(\"Task status: FAILED\", \"STATUS\")\n  1511→            return TaskResult(\n  1512→                success=False,\n  1513→                message=status.get(\"message\", \"Task failed\"),\n  1514→                duration_seconds=duration,\n  1515→                plan_modified=plan_modified\n  1516→            )\n  1517→        else:\n  1518→            verbose_log(\"Task status: UNKNOWN (no status file)\", \"STATUS\")\n  1519→            # No status file or unclear status - check if build passes\n  1520→            return TaskResult(\n  1521→                success=False,\n  1522→                message=\"No status file written by Claude\",\n  1523→                duration_seconds=duration\n  1524→            )\n  1525→\n  1526→    except subprocess.TimeoutExpired:\n  1527→        verbose_log(f\"TIMEOUT after {CLAUDE_TIMEOUT_SECONDS}s\", \"ERROR\")\n  1528→        return TaskResult(\n  1529→            success=False,\n  1530→            message=f\"Task timed out after {CLAUDE_TIMEOUT_SECONDS} seconds\",\n  1531→            duration_seconds=CLAUDE_TIMEOUT_SECONDS\n  1532→        )\n  1533→    except Exception as e:\n  1534→        verbose_log(f\"Exception: {type(e).__name__}: {e}\", \"ERROR\")\n  1535→        return TaskResult(\n  1536→            success=False,\n  1537→            message=f\"Error running Claude: {str(e)}\",\n  1538→            duration_seconds=time.time() - start_time\n  1539→        )\n  1540→\n  1541→\n  1542→def send_notification(plan: dict, subject: str, message: str) -> None:\n  1543→    \"\"\"Send a notification via Claude.\"\"\"\n  1544→    email = plan.get(\"meta\", {}).get(\"notification_email\", \"\")\n  1545→    if not email:\n  1546→        print(f\"[NOTIFICATION] {subject}: {message}\")\n  1547→        return\n  1548→\n  1549→    notification_prompt = f\"\"\"Send a notification to the user.\n  1550→\n  1551→Subject: {subject}\n  1552→Message: {message}\n  1553→\n  1554→Use the admin notification system or console log if notifications aren't configured.\n  1555→\"\"\"\n  1556→\n  1557→    try:\n  1558→        subprocess.run(\n  1559→            [*CLAUDE_CMD, \"--dangerously-skip-permissions\", \"--print\", notification_prompt],\n  1560→            capture_output=True,\n  1561→            text=True,\n  1562→            timeout=60\n  1563→        )\n  1564→    except Exception as e:\n  1565→        print(f\"[NOTIFICATION FAILED] {subject}: {message} (Error: {e})\")\n  1566→\n  1567→\n  1568→def update_section_status(section: dict) -> None:\n  1569→    \"\"\"Update section status based on task statuses.\"\"\"\n  1570→    tasks = section.get(\"tasks\", [])\n  1571→    if not tasks:\n  1572→        return\n  1573→\n  1574→    statuses = [t.get(\"status\") for t in tasks]\n  1575→\n  1576→    if all(s == \"completed\" for s in statuses):\n  1577→        section[\"status\"] = \"completed\"\n  1578→    elif any(s == \"in_progress\" for s in statuses):\n  1579→        section[\"status\"] = \"in_progress\"\n  1580→    elif any(s == \"failed\" for s in statuses):\n  1581→        section[\"status\"] = \"failed\"\n  1582→    elif all(s == \"pending\" for s in statuses):\n  1583→        section[\"status\"] = \"pending\"\n  1584→    else:\n  1585→        section[\"status\"] = \"in_progress\"\n  1586→\n  1587→\n  1588→def run_orchestrator(\n  1589→    plan_path: str,\n  1590→    dry_run: bool = False,\n  1591→    resume_from: Optional[str] = None,\n  1592→    single_task: bool = False,\n  1593→    verbose: bool = False,\n  1594→    parallel: bool = False,\n  1595→    skip_smoke: bool = False\n  1596→) -> None:\n  1597→    \"\"\"Main orchestrator loop.\"\"\"\n  1598→    global VERBOSE\n  1599→    VERBOSE = verbose\n  1600→\n  1601→    verbose_log(f\"Loading plan from: {plan_path}\", \"INIT\")\n  1602→    plan = load_plan(plan_path)\n  1603→    verbose_log(f\"Plan loaded successfully\", \"INIT\")\n  1604→\n  1605→    meta = plan.get(\"meta\", {})\n  1606→    default_max_attempts = meta.get(\"max_attempts_default\", DEFAULT_MAX_ATTEMPTS)\n  1607→\n  1608→    verbose_log(f\"Plan meta: {json.dumps(meta, indent=2, default=str)}\", \"INIT\")\n  1609→    verbose_log(f\"Sections in plan: {len(plan.get('sections', []))}\", \"INIT\")\n  1610→\n  1611→    # Initialize circuit breaker with configurable settings\n  1612→    circuit_breaker = CircuitBreaker(\n  1613→        threshold=meta.get(\"circuit_breaker_threshold\", DEFAULT_CIRCUIT_BREAKER_THRESHOLD),\n