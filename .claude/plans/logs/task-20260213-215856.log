r task_id, task_result in results.items():\n  1728→                    if task_result.success:\n  1729→                        worktree_path = worktree_paths.get(task_id)\n  1730→                        if worktree_path and worktree_path.exists():\n  1731→                            copy_ok, copy_msg, copied_files = copy_worktree_artifacts(\n  1732→                                worktree_path, task_id\n  1733→                            )\n  1734→                            if not copy_ok:\n  1735→                                merge_failures.append((task_id, copy_msg))\n  1736→                                print(f\"  [{task_id}] COPY FAILED: {copy_msg}\")\n  1737→                            else:\n  1738→                                all_copied_files.extend(copied_files)\n  1739→                                print(f\"  [{task_id}] {copy_msg}\")\n  1740→\n  1741→                # Stage and commit all copied artifacts in a single commit\n  1742→                if all_copied_files and not dry_run:\n  1743→                    try:\n  1744→                        # Separate existing files (add) from deleted files (rm)\n  1745→                        files_to_add = [f for f in all_copied_files if Path(f).exists()]\n  1746→                        files_to_rm = [f for f in all_copied_files if not Path(f).exists()]\n  1747→\n  1748→                        if files_to_add:\n  1749→                            subprocess.run(\n  1750→                                [\"git\", \"add\"] + files_to_add,\n  1751→                                capture_output=True, text=True, check=True\n  1752→                            )\n  1753→                        if files_to_rm:\n  1754→                            subprocess.run(\n  1755→                                [\"git\", \"rm\", \"--cached\", \"--ignore-unmatch\"] + files_to_rm,\n  1756→                                capture_output=True, text=True, check=True\n  1757→                            )\n  1758→\n  1759→                        task_ids_merged = [\n  1760→                            tid for tid, res in results.items() if res and res.success\n  1761→                        ]\n  1762→                        subprocess.run(\n  1763→                            [\"git\", \"commit\", \"-m\",\n  1764→                             f\"plan: Merge artifacts from parallel tasks \"\n  1765→                             f\"{', '.join(task_ids_merged)}\"],\n  1766→                            capture_output=True, text=True, check=False\n  1767→                        )\n  1768→                        verbose_log(\n  1769→                            f\"Committed {len(all_copied_files)} files from \"\n  1770→                            f\"{len(task_ids_merged)} parallel tasks\", \"MERGE\"\n  1771→                        )\n  1772→                    except subprocess.CalledProcessError as e:\n  1773→                        print(f\"  [WARNING] Failed to commit merged artifacts: {e.stderr}\")\n  1774→\n  1775→                # Cleanup worktrees\n  1776→                for task_id, worktree_path in worktree_paths.items():\n  1777→                    if worktree_path.exists():\n  1778→                        cleanup_worktree(worktree_path)\n  1779→\n  1780→                # Check if any parallel tasks hit rate limits\n  1781→                rate_limited_results = [\n  1782→                    r for r in results.values() if r and r.rate_limited\n  1783→                ]\n  1784→                if rate_limited_results:\n  1785→                    # Find the latest reset time among all rate-limited tasks\n  1786→                    reset_times = [r.rate_limit_reset_time for r in rate_limited_results if r.rate_limit_reset_time]\n  1787→                    latest_reset = max(reset_times) if reset_times else None\n  1788→                    print(f\"\\n[RATE LIMIT] {len(rate_limited_results)} parallel tasks hit rate limit\")\n  1789→\n  1790→                    # Reset all tasks in the group to pending 