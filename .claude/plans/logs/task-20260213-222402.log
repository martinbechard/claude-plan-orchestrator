tend(copied_files)\n  1808→                                print(f\"  [{task_id}] {copy_msg}\")\n  1809→\n  1810→                # Stage and commit all copied artifacts in a single commit\n  1811→                if all_copied_files and not dry_run:\n  1812→                    try:\n  1813→                        # Separate existing files (add) from deleted files (rm)\n  1814→                        files_to_add = [f for f in all_copied_files if Path(f).exists()]\n  1815→                        files_to_rm = [f for f in all_copied_files if not Path(f).exists()]\n  1816→\n  1817→                        if files_to_add:\n  1818→                            subprocess.run(\n  1819→                                [\"git\", \"add\"] + files_to_add,\n  1820→                                capture_output=True, text=True, check=True\n  1821→                            )\n  1822→                        if files_to_rm:\n  1823→                            subprocess.run(\n  1824→                                [\"git\", \"rm\", \"--cached\", \"--ignore-unmatch\"] + files_to_rm,\n  1825→                                capture_output=True, text=True, check=True\n  1826→                            )\n  1827→\n  1828→                        task_ids_merged = [\n  1829→                            tid for tid, res in results.items() if res and res.success\n  1830→                        ]\n  1831→                        subprocess.run(\n  1832→                            [\"git\", \"commit\", \"-m\",\n  1833→                             f\"plan: Merge artifacts from parallel tasks \"\n  1834→                             f\"{', '.join(task_ids_merged)}\"],\n  1835→                            capture_output=True, text=True, check=False\n  1836→                        )\n  1837→                        verbose_log(\n  1838→                            f\"Committed {len(all_copied_files)} files from \"\n  1839→                            f\"{len(task_ids_merged)} parallel tasks\", \"MERGE\"\n  1840→                        )\n  1841→                    except subprocess.CalledProcessError as e:\n  1842→                        print(f\"  [WARNING] Failed to commit merged artifacts: {e.stderr}\")\n  1843→\n  1844→                # Cleanup worktrees\n  1845→                for task_id, worktree_path in worktree_paths.items():\n  1846→                    if worktree_path.exists():\n  1847→                        cleanup_worktree(worktree_path)\n  1848→\n  1849→                # Check if any parallel tasks hit rate limits\n  1850→                rate_limited_results = [\n  1851→                    r for r in results.values() if r and r.rate_limited\n  1852→                ]\n  1853→                if rate_limited_results:\n  1854→                    # Find the latest reset time among all rate-limited tasks\n  1855→                    reset_times = [r.rate_limit_reset_time for r in rate_limited_results if r.rate_limit_reset_time]\n  1856→                    latest_reset = max(reset_times) if reset_times else None\n  1857→                    print(f\"\\n[RATE LIMIT] {len(rate_limited_results)} parallel tasks hit rate limit\")\n  1858→\n  1859→                    # Reset all tasks in the group to pending (don't count attempts)\n  1860→                    for section, task, _ in parallel_tasks:\n  1861→                        task[\"status\"] = \"pending\"\n  1862→                        task[\"attempts\"] = max(0, task.get(\"attempts\", 1) - 1)\n  1863→\n  1864→                    if not dry_run:\n  1865→                        save_plan(plan_path, plan)\n  1866→\n  1867→                    # Wait for rate limit reset\n  1868→                    should_continue = wait_for_rate_limit_reset(latest_reset)\n  1869→                    if not should_continue:\n  1870→                        if not dry_run:\n  1871→                            save_plan(plan_path, plan, commit=True,\n  1872→                                      commit_message=f\"plan: Rate limit wait aborted by user\")\n  1873→                        break\n  1874→                    continue  # Retry the parallel group\n  1875→\n  1876→                # Update task statuses in plan\n  1877→                for section, task, _ in parallel_tasks:\n  1878→                    task_id = task.get(\"id\")\n  1879→                    task_result = results.get(task_id)\n  1880→                    if task_result and task_result.success and task_id not in [f[0] for f in merge_failures]:\n  1881→                        task[\"status\"] = \"completed\"\n  1882→                        task[\"completed_at\"] = datetime.now().isoformat()\n  1883→                        task[\"result_message\"] = task_result.message\n  1884→                        tasks_completed += 1\n  1885→                        circuit_breaker.record_success()\n  1886→                        update_section_status(section)\n  1887→                    else:\n  1888→                        task[\"status\"] = \"pending\"  # Will retry\n  1889→                        if task_result:\n  1890→                            task[\"last_error\"] = task_result.message\n  1891→                        circuit_breaker.record_failure()\n  1892→\n  1893→                # Save plan after parallel execution\n  1894→                if not dry_run:\n  1895→                    save_plan(plan_path, plan, commit=True,\n  1896→                              commit_message=f\"plan: Parallel group '{group_name}' completed\")\n  1897→\n  1898→                # Check if there were merge failures requiring human intervention\n  1899→                if merge_failures:\n  1900→                    print(f\"\\n[WARNING] {len(merge_failures)} merge failures - manual intervention may be required\")\n  1901→                    for task_id, msg in merge_failures:\n  1902→                        print(f\"  - Task {task_id}: {msg}\")\n  1903→\n  1904→                if single_task:\n  1905→                    print(\"\\n[Single task mode - stopping after parallel group]\")\n  1906→                    break\n  1907→\n  1908→                continue  # Next iteration\n  1909→\n  1910→        # =====================================================================\n  1911→        # SEQUENTIAL EXECUTION (default)\n  1912→        # =====================================================================\n  1913→\n  1914→        # Find next task\n  1915→        verbose_log(\"Searching for next task...\", \"LOOP\")\n  1916→        result = find_next_task(plan)\n  1917→        verbose_log(f\"find_next_task returned: {result is not None}\", \"LOOP\")\n  1918→\n  1919→        if not result:\n  1920→            print(\"\\n=== All tasks completed! ===\")\n  1921→\n  1922→            # Run smoke tests as post-plan verification\n  1923→            if not dry_run and not skip_smoke:\n  1924→                smoke_ok = run_smoke_tests()\n  1925→                if not smoke_ok:\n  1926→                    print(\"\\n[WARNING] Smoke tests FAILED after plan completion!\")\n  1927→                    print(\"[WARNING] The plan completed but critical user paths may be broken.\")\n  1928→                    print(\"[WARNING] Run 'npx playwright test tests/SMOKE*.spec.ts --reporter=list' to investigate.\")\n  1929→                    send_notification(\n  1930→                        plan,\n  1931→                        \"Plan Completed - SMOKE TESTS FAILED\",\n  1932→                        f\"All tasks in '{meta.get('name')}' completed \"\n  1933→                        f\"(Completed: {tasks_completed}, Failed: {tasks_failed}) \"\n  1934→                        f\"but smoke tests FAILED. Critical paths may be broken!\"\n  1935→                    )\n  1936→                else:\n  1937→                    send_notification(\n  1938→                        plan,\n  1939→                        \"Plan Completed - All Verified\",\n  1940→                        f\"All tasks in '{meta.get('name')}' completed and smoke tests passed. \"\n